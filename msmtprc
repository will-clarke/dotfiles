defaults
tls on
auto_from on
logfile ~/.msmtp.log

account gmail
host smtp.gmail.com
tls on
tls_certcheck off
auth on
from wmmclarke@gmail.com
user wmmclarke@gmail.com
passwordeval gpg --batch --quiet --no-tty --passphrase "`security find-generic-password -a megalolz -s megalolz -w`"  -d ~/.password-store/email/wmmclarke@gmail.com-app.gpg
port 587

account snaptrip
host smtp.gmail.com
tls on
tls_certcheck off
auth on
from will.clarke@snaptrip.com
user will.clarke@snaptrip.com
passwordeval gpg --batch --quiet --no-tty --passphrase "`security find-generic-password -a megalolz -s megalolz -w`"  -d ~/.password-store/email/will.clarke@snaptrip.com.gpg
port 587

account default : gmail