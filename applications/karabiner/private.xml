<?xml version="1.0"?>
<root>
  <item>
    <name>Me: Swap Section Symbol(§) to Underscore(_)</name>
    <identifier>private.me_swap_section_symbol_with_underscore</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::DANISH_DOLLAR,  ModifierFlag::NONE,
      KeyCode::MINUS, ModifierFlag::SHIFT_L
    </autogen>
  </item>
  <item>
    <name>Me: Underscore to backtick</name>
    <identifier>private.me_underscore_to_backtick</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::MINUS, ModifierFlag::SHIFT_L,
      KeyCode::BACKQUOTE, ModifierFlag::NONE
    </autogen>
  </item>

  <item>
    <name>Me: ± to ~ </name>
    <identifier>private.me_plus_minus_to_squiggle</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::DANISH_DOLLAR,  ModifierFlag::SHIFT_L,
      KeyCode::BACKQUOTE, ModifierFlag::SHIFT_L
    </autogen>
  </item>

      <item>
      <name>Me: swap # and ` </name>
      <identifier>private.me_swap_hash_and_backtick</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::BACKQUOTE,  ModifierFlag::NONE,
        KeyCode::KEY_3, ModifierFlag::OPTION_L
      </autogen>
      <autogen>
        __KeyToKey__
        KeyCode::KEY_3, ModifierFlag::OPTION_L,
        KeyCode::BACKQUOTE,  ModifierFlag::NONE
      </autogen>
    </item>
</root>
